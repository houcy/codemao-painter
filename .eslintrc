    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true,
    "commonjs": true
  },
  "globals": {
    "fabric": true,
    "Vue": true,
    "template": true,
    "webpack": true,
    "jsColorPicker": true,
    "Colors": true
  },
  "extends": "airbnb/base",
  "plugins": [
    "react"
  ]
}